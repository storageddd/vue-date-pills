{
  "name": "vue-date-pills",
  "description": "Lightweight complete date pills component for filtering data by date range based on Vue 3 and DayJS",
  "version": "1.0.2",
  "private": false,
  "license": "MIT",
  "author": "Vadzim Veleshka <storaged@mail.ru>",
  "main": "dist/vue-date-pills.umd.js",
  "module": "dist/vue-date-pills.es.js",
  "browser": "dist/vue-date-pills.umd.js",
  "style": "./dist/style.css",
  "files": [
    "dist/*",
    "src/**/*",
    "README.md"
  ],
  "types": "src/types.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-date-pills.es.js",
      "require": "./dist/vue-date-pills.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storageddd/vue-date-pills.git"
  },
  "bugs": {
    "url": "https://github.com/storageddd/vue-date-pills/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:docs": "vite build --mode docs",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite serve --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  },
  "keywords": [
    "vue",
    "vue 3",
    "date",
    "picker",
    "pills",
    "datepicker",
    "date-picker",
    "datepills",
    "date-pills",
    "monthpicker",
    "year",
    "month",
    "library"
  ]
}
